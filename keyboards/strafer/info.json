            "layout": [
                {"label": "k00", "matrix": [0, 0] , "x":0, "y":  0},
                {"label": "k01", "matrix": [0, 1] , "x":1, "y":0.5},
                {"label": "k02", "matrix": [0, 2] , "x":2, "y":0.5},
                {"label": "k03", "matrix": [0, 3] , "x":3, "y":0.5},
