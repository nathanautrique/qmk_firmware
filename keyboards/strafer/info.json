{
    "keyboard_name": "strafer",
    "maintainer": "nathanautrique",
    "manufacturer": "dBounced",
    "url": "https://dbounced.com",
    "bootloader": "atmel-dfu",

    "diode_direction": "COL2ROW",
    "matrix_pins": {
        "rows": ["C7", "C6", "B6", "B5", "B4"],
        "cols": ["F0", "F1", "F4", "F5", "D6", "D4"]
    },

    "processor": "atmega32u4",
    "usb": {
        "vid": "0x6442",
        "pid": "0x0001" ,
        "device_version": "0.0.1",
        "max_power": 500
    },

    "features": {
        "bootmagic": true,
        "command": false,
        "console": false,
        "extrakey": true,
        "mousekey": true,
        "nkro": true,
        "rgb_matrix": true,
        "rgblight": false,
        "audio": false
    },

    "build":{
        "debounce_type":"asym_eager_defer_pk",
        "lto":true
    },
    "debounce": 8,

    "ws2812": {
        "pin": "F1"
    },

    "rgb_matrix": {
        "driver": "ws2812",
        "max_brightness": 150,
        "center_point": [93,25],
        "animations": {
            "alphas_mods": true,
            "gradient_up_down": true,
            "gradient_left_right": true,
            "breathing": true,
            "band_sat": true,
            "band_val": true,
            "band_pinwheel_sat": true,
            "band_pinwheel_val": true,
            "band_spiral_sat": true,
            "band_spiral_val": true,
            "cycle_all": true,
            "cycle_left_right": true,
            "cycle_up_down": true,
            "rainbow_moving_chevron": true,
            "cycle_out_in": true,
            "cycle_out_in_dual": true,
            "cycle_pinwheel": true,
            "cycle_spiral": true,
            "dual_beacon": true,
            "rainbow_beacon": true,
            "rainbow_pinwheels": true,
            "raindrops": true,
            "jellybean_raindrops": true,
            "hue_breathing": true,
            "hue_pendulum": true,
            "hue_wave": true,
            "pixel_rain": true,
            "pixel_flow": true,
            "pixel_fractal": true,
            "typing_heatmap": true,
            "digital_rain": true,
            "solid_reactive_simple": true,
            "solid_reactive": true,
            "solid_reactive_wide": true,
            "solid_reactive_multiwide": true,
            "solid_reactive_cross": true,
            "solid_reactive_multicross": true,
            "solid_reactive_nexus": true,
            "solid_reactive_multinexus": true,
            "splash": true,
            "multisplash": true,
            "solid_splash": true,
            "solid_multisplash": true
        }
    },

    "layouts": {
